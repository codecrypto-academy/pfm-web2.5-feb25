{
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "lib": ["dom", "es6"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      }
    },
    "include": [
      "frontback/**/*",
      "lib/**/*",
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx"
, "paralelcode/src"    ],
    "exclude": [
      "node_modules",
      "dist",
      ".next"
    ],
    "types": ["jest", "node", "jest-fetch-mock"]

  }